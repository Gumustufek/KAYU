#include <stdio.h>
int main(){

    int a , b , c;

    printf("Lutfen aralarinda bosluk birakarak 3 tam sayi giriniz: ");
    scanf("%d %d %d", &a , &b , &c);

    if(a>b && a>c){
        printf("En buyuk sayi: %d", a);
    }
    else if(b>a && b>c){
        printf("En buyuk sayi: %d", b);
    }
    else if(c>a && c>b){
        printf("En buyuk sayi: %d", c);
    }

    return 0;
}
